{
  "name": "portald",
  "version": "0.3.0",
  "description": "Portald SDK and CLI for merchant integration with AI agent payments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "portald": "dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk/index.js",
      "require": "./dist/sdk/index.js",
      "types": "./dist/sdk/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "portald",
    "ai",
    "agents",
    "payments",
    "authorization",
    "approval",
    "stripe",
    "connect"
  ],
  "author": "Charles Cushman",
  "license": "MIT",
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.9.2"
  }
}
